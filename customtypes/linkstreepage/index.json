{
  "format": "custom",
  "id": "linkstreepage",
  "label": "LinkstreePage",
  "repeatable": true,
  "status": true,
  "json": {
    "Main": {
      "uid": {
        "config": {
          "label": "UID"
        },
        "type": "UID"
      },
      "pagetitle": {
        "type": "Text",
        "config": {
          "label": "pageTitle",
          "placeholder": ""
        }
      },
      "pagedescription": {
        "type": "StructuredText",
        "config": {
          "label": "pageDescription",
          "placeholder": "",
          "allowTargetBlank": true,
