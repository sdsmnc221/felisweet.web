      },
      "linksillustration": {
        "type": "Image",
        "config": {
          "label": "linksIllustration",
          "constraint": {},
          "thumbnails": []
        }
      },
      "links": {
        "type": "Group",
        "config": {
          "label": "links",
          "repeat": true,
          "fields": {
            "linklabel": {
              "type": "Text",
              "config": {
                "label": "linklabel",
                "placeholder": ""
              }
            },
            "linkurl": {
              "type": "Link",
              "config": {
                "label": "linkUrl",
                "placeholder": "",
                "allowTargetBlank": true,
                "select": null
              }
            }
          }
        }
      }
