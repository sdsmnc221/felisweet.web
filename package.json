    "core-js": "^3.19.3",
    "gsap": "^3.11.0",
    "howler": "^2.2.3",
    "nuxt": "^2.17.3",
    "paper": "^0.12.15",
    "scratchcard-js": "^1.4.10",
    "simplex-noise": "^3.0.1",
    "vue": "^2.6.14",
    "vue-inline-svg": "^2.1.1",
    "vue-server-renderer": "^2.6.14",
    "vue-template-compiler": "^2.6.14",
    "webpack": "^4.46.0"
